<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/_coordinator"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/root_tabs"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear_notab"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:paddingLeft="8dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<TextView
					android:id="@+id/sadface_text"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="8dp"
					android:paddingRight="8dp"
					android:text=":("
					android:textSize="60sp"
					 
					android:fontFamily="@font/inter_variable" />
				<ImageView
					android:id="@+id/incognito_image"
					android:layout_width="80dp"
					android:layout_height="80dp"
					android:layout_marginLeft="8dp"
					android:layout_marginTop="8dp"
					android:layout_marginRight="8dp"
					 
					android:scaleType="fitCenter"
					android:src="@drawable/ic_incognito_circle_24" />
				<TextView
					android:id="@+id/notab_text"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:paddingBottom="8dp"
					android:gravity="center_horizontal"
					android:text="Looks like you don't have any tab open.\nClick the plus (+) button to open a new tab!"
					android:textSize="14sp"
					 
					android:fontFamily="@font/inter_variable" />
			</LinearLayout>
			<ListView
				android:id="@+id/tabs_list"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:choiceMode="none"
				android:layout_weight="1"
				android:stackFromBottom="true" />
			<LinearLayout
				android:id="@+id/linear_t"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingLeft="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/settings_button"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:layout_margin="6dp"
					android:padding="4dp"
					 
					android:scaleType="centerInside"
					android:src="@drawable/ic_settings_24"
					android:background="?attr/selectableItemBackgroundBorderless" />
				<LinearLayout
					android:id="@+id/spacing_linear"
					android:layout_width="0dp"
					android:layout_height="1dp"
					android:padding="8dp"
					android:orientation="horizontal"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/incognito_button"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:layout_margin="6dp"
					android:padding="4dp"
					 
					android:scaleType="centerInside"
					android:src="@drawable/ic_incognito_24"
					android:background="?attr/selectableItemBackgroundBorderless" />
				<LinearLayout
					android:id="@+id/tabscounter_linear1"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:layout_margin="6dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical"
					android:background="?attr/selectableItemBackgroundBorderless">
					<TextView
						android:id="@+id/tabscounter_text1"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="0"
						android:textSize="12sp"
						android:textStyle="bold"
						android:fontFamily="@font/inter_variable"
						android:background="@drawable/ic_check_box_outline_blank_24"
						android:textColor="?attr/colorControlNormal" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/root_webview"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<androidx.coordinatorlayout.widget.CoordinatorLayout
				android:id="@+id/coordinatorlayout"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_weight="1">
				<LinearLayout
					android:id="@+id/wv_placeholder"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="vertical" />
				<ListView
					android:id="@+id/query_list"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:choiceMode="none"
					android:layout_gravity="bottom"
					android:stackFromBottom="true" />
			</androidx.coordinatorlayout.widget.CoordinatorLayout>
			<LinearLayout
				android:id="@+id/linear_findbar"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<EditText
					android:id="@+id/findbar_input"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:textSize="16sp"
					 
					android:hint="Find..."
					 
					android:layout_weight="1"
					android:fontFamily="@font/inter_variable" />
				<ImageView
					android:id="@+id/findbar_findprev_button"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:layout_margin="6dp"
					android:padding="4dp"
					 
					android:scaleType="centerInside"
					android:enabled="false"
					android:alpha="0.5"
					android:src="@drawable/ic_expand_less_24"
					android:background="?attr/selectableItemBackgroundBorderless" />
				<ImageView
					android:id="@+id/findbar_findnext_button"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:layout_margin="6dp"
					android:padding="4dp"
					 
					android:scaleType="centerInside"
					android:enabled="false"
					android:alpha="0.5"
					android:src="@drawable/ic_expand_more_24"
					android:background="?attr/selectableItemBackgroundBorderless" />
				<ImageView
					android:id="@+id/findbar_close_button"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:layout_margin="6dp"
					android:padding="4dp"
					 
					android:scaleType="centerInside"
					android:src="@drawable/ic_close_24"
					android:background="?attr/selectableItemBackgroundBorderless" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear_chromebar"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical">
				<ProgressBar
					android:id="@+id/wv_progressbar"
					android:layout_width="match_parent"
					android:layout_height="8dp"
					android:indeterminate="false"
					style="?android:progressBarStyleHorizontal" />
				<LinearLayout
					android:id="@+id/linear_searchbar"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:paddingBottom="8dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<EditText
						android:id="@+id/urlbar"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:textSize="16sp"
						 
						android:hint="Search or type web url"
						 
						android:singleLine="true"
						android:imeOptions="actionSearch"
						android:layout_weight="1"
						android:fontFamily="@font/inter_variable" />
					<ImageView
						android:id="@+id/refresh_button"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:layout_margin="6dp"
						android:padding="4dp"
						 
						android:scaleType="centerInside"
						android:src="@drawable/ic_refresh_24"
						android:background="?attr/selectableItemBackgroundBorderless" />
					<ImageView
						android:id="@+id/forward_button"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:layout_margin="6dp"
						android:padding="4dp"
						 
						android:scaleType="centerInside"
						android:src="@drawable/ic_arrow_forward_24"
						android:background="?attr/selectableItemBackgroundBorderless" />
					<LinearLayout
						android:id="@+id/tabscounter_linear2"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:layout_margin="6dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:background="?attr/selectableItemBackgroundBorderless">
						<TextView
							android:id="@+id/tabscounter_text2"
							android:layout_width="30dp"
							android:layout_height="30dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="0"
							android:textSize="12sp"
							android:textStyle="bold"
							android:fontFamily="@font/inter_variable"
							android:background="@drawable/ic_check_box_outline_blank_24"
							android:textColor="?attr/colorControlNormal" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
	<com.google.android.material.floatingactionbutton.FloatingActionButton
android:src="@drawable/ic_add_24"
		android:id="@+id/_fab"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_margin="8dp"
		android:layout_gravity="center_horizontal|bottom" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>